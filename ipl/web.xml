<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>ipl</display-name>
  <welcome-file-list>
    <welcome-file>iplsignup.html</welcome-file>
    
  </welcome-file-list>
  <session-config>
  <session-timeout>1</session-timeout></session-config>
  <servlet>
  <servlet-name>serv</servlet-name>
  <servlet-class>ipl.IplSignup</servlet-class>
  
   </servlet>
  
  <servlet-mapping>
  <servlet-name>serv</servlet-name>
  <url-pattern>/iplsignup</url-pattern>
  </servlet-mapping>
  
 

<servlet>
  <servlet-name>serv1</servlet-name>
  <servlet-class>ipl.iplogin</servlet-class>
   
  </servlet>
  <servlet-mapping><servlet-name>serv1</servlet-name>
  <url-pattern>/Register</url-pattern>
  <url-pattern>/loginform</url-pattern>
  </servlet-mapping>
  
</web-app>